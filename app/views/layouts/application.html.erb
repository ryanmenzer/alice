<!DOCTYPE html>
<html>
<head>
  <title>Alice</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <script>
    System.config({
      packages: {
        app: {
          format: 'register',
          defaultExtension: 'js'
        }
      }
    });
    System.import('/app/assets/javascripts/boot')
          .then(null, console.error.bind(console));
  </script>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= yield %>
</body>
</html>
